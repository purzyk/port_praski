<picture data-aos="fade-up" data-aos-duration="800" data-aos-delay="100">
	<source srcset="{{img_src|resize(width,height)|towebp}}" type="image/webp">
	<source srcset="{{img_src|resize(width,height)}}" type="image/jpeg">
	<img src="{{img_src|resize(width,height)}}" {% if image.description %} alt="{{image.description}}" {% endif %}>
</picture>
